.box{
  position: relative;
  margin: 0 auto;
  height: 100%;
  width: 650rpx;
}
.title{
  position: relative;
  top: 0;
  font-size: 30rpx;
  margin: 5rpx auto;
  width: 250rpx;
  text-align: center;
}

.logo{
  text-align: center;
  line-height: 275rpx;
  background:#83f0ff;
}
.main{
  width: 100%;
  padding-top:250rpx;
}

.background {
  width: 100%;
  height: 100%;
  position:fixed; 
  background-size:100% 100%;
  z-index: -1;
  }

  .menu_cell{
    display: flex;
    text-align: center;
    justify-content: center;
    align-items: center;
    flex-direction: column;
    padding: 30rpx;
    font-size:12px;
  }
  .menu_cell image{
    width: 80rpx;
    height: 80rpx;
  }
  .menu_cell_block{
    color: #9e9696;
    margin-top: 10rpx;
    width: 60rpx;
    height: 30rpx;
  }
.menuBlock{
  display: grid;
  grid-template-columns: repeat(3,33.3%);
  grid-template-rows: repeat(2,50%);
  position: relative;
  margin:100rpx auto;
  width: 650rpx;
  height: 450rpx;
  border-radius: 40rpx;
  box-shadow: 0rpx 0rpx 10rpx 6rpx rgba(224,224,224,0.77);
  background-color: white;
}