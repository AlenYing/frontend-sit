.swiper {
  width: 100%;
  height: 292.4rpx;
}
/*高宽比约为0.42*/
.box {
  position: relative;
  margin: 0 auto;
  height: 100%;
  width: 680rpx;
}



.main {
  width: 100%;
  padding-top: var(--padding-size);
}

.background {
  width: 100%;
  height: 100%;
  position: fixed;
  background-size: 100% 100%;
  z-index: -1;
}

.menu_cell {
  display: flex;
  text-align: center;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  padding-top: 20px;
  font-size: var(--font-size);
}

.menu_cell:nth-child(1) {
  border-top-left-radius: 10px;
}

.menu_cell:nth-child(3) {
  border-top-right-radius: 10px;
}

.menu_cell:nth-child(4) {
  border-bottom-left-radius: 10px;
}

.menu_cell:nth-child(6) {
  border-bottom-right-radius: 10px;
}

.menu_cell:active {
  background-color: #e3e3e3;
}

.menu_cell image {
  width: 32px;
  height: 32px;
}

.menu_cell_text {
  color: #9e9696;
  margin-top: 10px;
  width: 60px;
  height: 30px;
}

.menuBlock {
  display: grid;
  grid-template-columns: repeat(3, 33.3%);
  grid-template-rows: repeat(2, 50%);
  position: relative;
  margin: 15vh auto;
  width: 680rpx;
  height: 453rpx;
  border-radius: 10px;
  box-shadow: 0px 0px 10px 6px rgba(224, 224, 224, 0.37);
  background-color: white;
}

@keyframes living {
  0% {
    transaform: scale(1);
  }
  25%{
    transform: scale(1.4);
  }
  50% {
    transform: scale(1);
  }
  80%{
    transform: scale(1.3);
  }
  100% {
    transform: scale(1);
  }
}